		<OutputPath>..\..\Bin\</OutputPath>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../logo.jpg">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
